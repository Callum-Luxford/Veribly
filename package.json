{
  "name": "certifyme-demo",
  "version": "1.0.0",
  "scripts": {
    "build:css": "tailwindcss -i ./client/css/tailwind.css -o ./client/css/root.css",
    "watch:css": "tailwindcss -i ./client/css/tailwind.css -o ./client/css/root.css --watch",
    "start": "node server/app.js",
    "dev": "concurrently \"npm run start\" \"npm run watch:css\""
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.4",
    "tailwindcss": "^3.4.17"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "bcrypt": "^6.0.0",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^5.1.0",
    "dotenv": "^16.5.0",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-ejs-layouts": "^2.5.1",
    "express-session": "^1.18.1",
    "gitignore": "^0.7.0",
    "mongodb": "^6.17.0",
    "mongoose": "^8.15.1",
    "particles.js": "^2.0.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0"
  }
}
